{
  "version": "1.1",
  "name": "Music OGA",
  "short_name": "music-oga",
  "description": "Original Gaia Music app",
  "orientation": "default",
  "start_url": "/index.html",
  "icons": [
    {
      "src": "/style/icons/default_84.png",
      "type": "image/png",
      "sizes": "84x84"
    },
    {
      "src": "/style/icons/default_126.png",
      "type": "image/png",
      "sizes": "126x126"
    },
    {
      "src": "/style/icons/default_142.png",
      "type": "image/png",
      "sizes": "142x142"
    },
    {
      "src": "/style/icons/default_189.png",
      "type": "image/png",
      "sizes": "189x189"
    },
    {
      "src": "/style/icons/default_284.png",
      "type": "image/png",
      "sizes": "284x284"
    }
  ],
  "b2g_features": {
    "core": true,
    "developer": {
      "name": "The Gaia Team",
      "url": "https://github.com/mozilla-b2g/gaia"
    },
    "permissions": {
      "themeable": {},
      "device-storage:music": {
        "access": "readwrite"
      },
      "device-storage:pictures": {
        "access": "readwrite"
      },
      "settings": {
        "access": "readonly"
      },
      "bluetooth": {},
      "audio-channel-content": {},
      "nfc-share": {},
      "moz-extremely-unstable-and-will-change-webcomponents": {}
    },
    "activities": {
      "pick": {
        "filters": {
          "type": [
            "audio/*",
            "audio/mpeg",
            "audio/ogg",
            "audio/mp4",
            "audio/flac"
          ]
        },
        "disposition": "inline",
        "returnValue": true,
        "href": "/index.html#pick"
      },
      "open": {
        "filters": {
          "type": [
            "audio/mpeg",
            "audio/ogg",
            "audio/mp4",
            "audio/amr",
            "audio/flac"
          ]
        },
        "disposition": "inline",
        "returnValue": true,
        "href": "/open.html"
      }
    },
    "messages": [
      {
        "media-button": "/index.html"
      }
    ]
  }
}